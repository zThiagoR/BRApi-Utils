{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "standard", "eslint:recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "space-before-function-paren": ["error", "never"]
  }
}
